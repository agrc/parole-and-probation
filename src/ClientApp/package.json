{
  "name": "adult-parole-and-probation",
  "homepage": "/",
  "version": "1.17.5",
  "bugs": "https://github.com/agrc/parole-and-probation/issues",
  "author": {
    "name": "Steve Gourley",
    "email": "sgourley@utah.gov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/parole-and-probation.git"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@agrc/dart-board": "^3.3.0",
    "@agrc/layer-selector": "^4.0.4",
    "@arcgis/core": "^4.18.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^4.6.0",
    "clsx": "^1.1.1",
    "downshift": "^6.1.0",
    "file-saver": "^2.0.5",
    "immer": "^8.0.1",
    "lodash": "^4.17.20",
    "lodash.isequal": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-oidc": "^1.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "reactstrap": "^8.9.0",
    "use-immer": "^0.4.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "build-react:stage": "./node_modules/.bin/env-cmd -f ./.env.stage npm run build",
    "build-storybook": "build-storybook -s public",
    "build:prod": "dotnet publish ../parole.csproj -c Release /p:EnvironmentName=Production",
    "build:stage": "dotnet publish ../parole.csproj -c Release -r win10-x64 --self-contained false /p:EnvironmentName=Staging",
    "build": "react-scripts build",
    "copy:arcgis": "npm run copy:public && npm run copy:build",
    "copy:public": "ncp ./node_modules/@arcgis/core/assets ./public/assets",
    "copy:build": "ncp ./node_modules/@arcgis/core/assets ./build/assets",
    "eject": "react-scripts eject",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "lint": "eslint src",
    "release": "standard-version",
    "start:all": "dotnet run -p ../parole.csproj /p:EnvironmentName=Development",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test": "react-scripts test",
    "wonkavise:clean": "wonkavision clean ./build ./../bin ./../../deploy",
    "wonkavise:extract": "env-cmd -e stage wonkavision unzip parole deploy.zip --extra-command \"rm app_offline.htm\"",
    "wonkavise:ship": "env-cmd -e stage wonkavision ship .\\..\\..\\deploy\\deploy.zip parole && env-cmd -e stage wonkavision ship ../app_offline.htm parole",
    "wonkavise:zip": "wonkavision zip ../bin/Release/net5.0/win10-x64/publish ./../../deploy deploy.zip",
    "wonkavise": "npm run wonkavise:clean && npm run build:stage && npm run wonkavise:zip && npm run wonkavise:ship && npm run wonkavise:extract"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "devDependencies": {
    "@agrc/wonkavision": "^1.0.2",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addon-storyshots": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^7.2.0",
    "fetch-mock": "^8.3.2",
    "ncp": "^2.0.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "infile": "../../CHANGELOG.md"
  }
}
