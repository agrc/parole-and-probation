{
  "name": "adult-parole-and-probation",
  "version": "1.21.2",
  "bugs": "https://github.com/agrc/parole-and-probation/issues",
  "author": {
    "name": "Steve Gourley",
    "email": "sgourley@utah.gov"
  },
  "homepage": "https://fieldmap.udc.utah.gov/",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/parole-and-probation.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "build": "vite build",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "vite",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ./.gitignore",
    "release": "standard-version",
    "start:all": "env-cmd --environments development dotnet run --project ../parole.csproj",
    "start": "vite",
    "sw": "node build-service-worker.js",
    "storybook": "start-storybook -p 9009 -s public",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "prettier/prettier": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "@agrc/helpers": "^1.3.1",
    "@arcgis/core": "^4.23.7",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "bootstrap": "^4.6.0",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "file-saver": "^2.0.5",
    "immer": "^9.0.14",
    "ky": "^0.30.0",
    "lodash.isequal": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-loading-skeleton": "^3.1.0",
    "react-navigator-status": "^0.1.2",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^9.0.3",
    "use-immer": "^0.7.0",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.3",
    "workbox-broadcast-update": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-google-analytics": "^6.5.3",
    "workbox-navigation-preload": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-streams": "^6.5.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@storybook/react": "^6.5.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^14.2.0",
    "@vitejs/plugin-react": "^1.3.2",
    "cpy-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.5.0",
    "fetch-mock": "^9.11.0",
    "prettier": "^2.6.2",
    "query-string": "^7.1.1",
    "standard-version": "^9.5.0",
    "vite": "^2.9.9",
    "vitest": "^0.13.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "infile": "../../CHANGELOG.md"
  }
}
