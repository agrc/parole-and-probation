{
  "name": "adult-parole-and-probation",
  "homepage": "https://test2.mapserv.utah.gov/app",
  "version": "1.11.0",
  "bugs": "https://github.com/agrc/parole-and-probation/issues",
  "author": {
    "name": "Steve Gourley",
    "email": "sgourley@utah.gov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/parole-and-probation.git"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "downshift": "^3.2.12",
    "esri-loader": "^2.10.0",
    "immer": "^3.2.0",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-fast-compare": "^2.0.4",
    "react-oidc": "^1.0.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "reactstrap": "^8.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:all": "dotnet run -p ../app.csproj",
    "release": "standard-version",
    "build": "react-scripts build",
    "build-react:stage": "./node_modules/.bin/env-cmd -f ./.env.stage npm run build",
    "build:stage": "ASPNETCORE_ENVIRONMENT=Staging dotnet publish ../app.csproj -c Release",
    "build:prod": "ASPNETCORE_ENVIRONMENT=Production dotnet publish ../app.csproj -c Release",
    "deploy:stage": "grunt clean:aspnet && npm run build:stage && grunt deploy:stage",
    "deploy:prod": "grunt clean:aspnet && npm run build && grunt deploy:prod",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "cz-conventional-changelog": "^3.0.2",
    "env-cmd": "^9.0.3",
    "fetch-mock": "^7.3.9",
    "grunt": "^1.0.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-ssh": "^0.12.9",
    "load-grunt-tasks": "^5.1.0",
    "standard-version": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "infile": "../../CHANGELOG.md"
  }
}
