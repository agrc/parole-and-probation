{
  "name": "adult-parole-and-probation",
  "version": "1.21.2",
  "bugs": "https://github.com/agrc/parole-and-probation/issues",
  "author": {
    "name": "Steve Gourley",
    "email": "sgourley@utah.gov"
  },
  "homepage": "https://fieldmap.udc.utah.gov/",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/parole-and-probation.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-storybook": "storybook build",
    "build": "vite build",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "vite",
    "format": "prettier --write src --ignore-path ../.gitignore",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ../.gitignore",
    "preview": "vite preview",
    "release": "standard-version",
    "start:all": "dotnet run --project ../app.csproj",
    "start": "vite",
    "storybook": "storybook dev -p 9009 --disable-telemetry",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "prettier/prettier": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.29.10",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^5.28.14",
    "@ugrc/dart-board": "^5.0.8",
    "@ugrc/layer-selector": "^6.2.3",
    "@ugrc/utilities": "^2.2.1",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.0",
    "downshift": "^9.0.4",
    "file-saver": "^2.0.5",
    "immer": "^10.0.4",
    "ky": "^1.2.3",
    "lodash.isequal": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-loading-skeleton": "^3.4.0",
    "react-navigator-status": "^0.1.2",
    "react-router-dom": "^6.22.3",
    "reactstrap": "^9.2.2",
    "use-immer": "^0.9.0",
    "web-vitals": "^3.5.2",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cpy-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fetch-mock": "^9.11.0",
    "happy-dom": "^14.4.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.14",
    "query-string": "^9.0.0",
    "standard-version": "^9.5.0",
    "storybook": "^8.0.5",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^0.19.7",
    "vitest": "^1.4.0",
    "workbox-build": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "infile": "../../CHANGELOG.md"
  }
}
