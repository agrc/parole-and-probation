  "bugs": "https://github.com/agrc/parole-and-probation/issues",
  "author": {
    "name": "Steve Gourley",
    "email": "sgourley@utah.gov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/parole-and-probation.git"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.1.3",
    "downshift": "^3.2.10",
    "esri-loader": "^2.10.0",
    "immer": "^3.1.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-fast-compare": "^2.0.4",
    "react-oidc": "^0.3.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "reactstrap": "^8.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "release": "standard-version",
    "build": "react-scripts build",
    "build-react:stage": "./node_modules/.bin/env-cmd -f ./.env.stage npm run build",
    "build:stage": "ASPNETCORE_ENVIRONMENT=Staging dotnet publish ../app.csproj -c Release",
    "build:prod": "ASPNETCORE_ENVIRONMENT=Production dotnet publish ../app.csproj -c Release",
    "deploy:stage": "grunt clean:aspnet && npm run build:stage && grunt deploy:stage",
    "deploy:prod": "grunt clean:aspnet && npm run build && grunt deploy:prod",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "cz-conventional-changelog": "^2.1.0",
    "env-cmd": "^9.0.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "fetch-mock": "^7.3.7",
    "grunt": "^1.0.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-ssh": "^0.12.9",
    "load-grunt-tasks": "^5.0.0",
    "standard-version": "^6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "infile": "../../CHANGELOG.md"
  }
}
