{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.GoogleCloudLogging",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      },
      "Enrich": [
        "FromLogContext"
      ]
    },
    "WriteTo": [
      "Console",
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "logName": "parole-api",
          "useSourceContextAsLogName": false,
          "projectID": "ut-dts-agrc-parole-dev",
          "useJsonOutput": true,
          "resourceType": "global",
          "resourceLabels": {
            "project_id": "ut-dts-agrc-parole-dev"
          },
          "labels": {
            "configuration": "development"
          },
          "serviceName": "parole-api",
          "serviceVersion": "1.0.0"
        }
      }
    ]
  },
  "ArcGIS": {
    "username": "user",
    "password": "",
    "host": "https://domain/arcgis/",
    "scheme": "https",
    "pathBase": "/arcgis",
    "pathString": "/rest/services/.../MapServer"
  }
}
