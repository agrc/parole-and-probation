{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.ReverseProxy.Service.Proxy": "Warning",
        "System": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      "Console"
    ]
  },
  "AllowedHosts": "*",
  "ReverseProxy": {},
  "ArcGIS": {
    "scheme": "https",
    "pathBase": "/arcgis",
    "pathString": "/rest/services/DOC/Offenders/MapServer"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Email": {
    "apikey": "",
    "pickup": "c:/dev/.mailbox",
    "testing": true
  }
}
