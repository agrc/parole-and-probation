{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.ReverseProxy.Service.Proxy": "Warning",
        "System": "Warning"
      }
    },
    "Using": ["Serilog.Sinks.Console"],
    "Enrich": ["FromLogContext"],
    "WriteTo": ["Console"]
  },
  "AllowedHosts": "*",
  "ReverseProxy": {},
  "ArcGIS": {
    "scheme": "https",
    "pathBase": "/arcgis",
    "pathString": "/rest/services/DOC/Offenders/MapServer"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Email": {
    "smtp": "send.state.ut.us",
    "pickup": "c:/dev/_mailbox",
    "testing": true
  }
}
