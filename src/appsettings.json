{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.ReverseProxy.Service.Proxy": "Warning",
        "System": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      "Console"
    ]
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "arcgis",
        "ClusterId": "arcgis",
        "AuthorizationPolicy": "Cookies",
        "Match": {
          "Path": "/mapserver/{*remainder}"
        },
        "Transforms": [
          {
            "PathPattern": "{remainder}"
          },
          {
            "RequestHeadersCopy": "false"
          }
        ]
      }
    ]
  },
  "ArcGIS": {
    "scheme": "https",
    "pathBase": "/arcgis",
    "pathString": "/rest/services/DOC/Offenders/MapServer"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Email": {
    "smtp": "send.state.ut.us",
    "pickup": "c:/dev/_mailbox",
    "testing": true
  }
}
